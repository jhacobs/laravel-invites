{
    "name": "jhacobs/invites",
    "description": "Invite users to your laravel application",
    "keywords": [
        "laravel",
        "invites",
        "laravel-invites"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Aretz",
            "email": "jacob.aretz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.9",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Jhacobs\\Invites\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jhacobs\\Invites\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jhacobs\\Invites\\InviteServiceProvider"
            ],
            "aliases": {
                "Invite": "Jhacobs\\Invites\\Facades\\Invite"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
